   "source": [
    "def infinite_fib():\n",
    "    a, b = 0, 1\n",
    "    while True:\n",
    "        yield a\n",
    "        a, b = b, a + b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "64166840-8c1b-435d-bc0c-494eb9b14ba0",
   "metadata": {},
